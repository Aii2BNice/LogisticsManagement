package com.neusoft.finance.invoicedj.model;

/**
 * 允许登记的发票 pojo类
 * @author new
 *
 */
public class InvoiceCXDJ {
	private Integer id;
	private Double price;
	
	public InvoiceCXDJ() {
		super();
	}

	public InvoiceCXDJ(Integer id, Double price) {
		super();
		this.id = id;
		this.price = price;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "invoiceCXDJ [id=" + id + ", price=" + price + "]";
	}
	
}
