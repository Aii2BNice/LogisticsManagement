package com.neusoft.depotmanage.selectform.model;
/**
 * 出入库单表实体类
 */
import java.util.Date;

public class IoDepotForm {

	private Integer iodepotId;
	private Integer ioAllocateId;
	private Date ioDepotDate;
	private Integer taskId;
	private Integer allocateId;
	private Integer ioDepotType;
	private Integer depotId;
	private Integer shopId;
	private Integer shopAmount;
	
	
	
	public IoDepotForm() {
		super();
	}

	public IoDepotForm(Integer iodepotId, Integer ioAllocateId, Date ioDepotDate, Integer taskId, Integer allocateId,
			Integer ioDepotType, Integer depotId, Integer shopId, Integer shopAmount) {
		super();
		this.iodepotId = iodepotId;
		this.ioAllocateId = ioAllocateId;
		this.ioDepotDate = ioDepotDate;
		this.taskId = taskId;
		this.allocateId = allocateId;
		this.ioDepotType = ioDepotType;
		this.depotId = depotId;
		this.shopId = shopId;
		this.shopAmount = shopAmount;
	}

	public Integer getIodepotId() {
		return iodepotId;
	}

	public void setIodepotId(Integer iodepotId) {
		this.iodepotId = iodepotId;
	}

	public Integer getIoAllocateId() {
		return ioAllocateId;
	}

	public void setIoAllocateId(Integer ioAllocateId) {
		this.ioAllocateId = ioAllocateId;
	}

	public Date getIoDepotDate() {
		return ioDepotDate;
	}

	public void setIoDepotDate(Date ioDepotDate) {
		this.ioDepotDate = ioDepotDate;
	}

	public Integer getTaskId() {
		return taskId;
	}

	public void setTaskId(Integer taskId) {
		this.taskId = taskId;
	}

	public Integer getAllocateId() {
		return allocateId;
	}

	public void setAllocateId(Integer allocateId) {
		this.allocateId = allocateId;
	}

	public Integer getIoDepotType() {
		return ioDepotType;
	}

	public void setIoDepotType(Integer ioDepotType) {
		this.ioDepotType = ioDepotType;
	}

	public Integer getDepotId() {
		return depotId;
	}

	public void setDepotId(Integer depotId) {
		this.depotId = depotId;
	}

	public Integer getShopId() {
		return shopId;
	}

	public void setShopId(Integer shopId) {
		this.shopId = shopId;
	}

	public Integer getShopAmount() {
		return shopAmount;
	}

	public void setShopAmount(Integer shopAmount) {
		this.shopAmount = shopAmount;
	}

	@Override
	public String toString() {
		return "IoDepotForm [iodepotId=" + iodepotId + ", ioAllocateId=" + ioAllocateId + ", ioDepotDate=" + ioDepotDate
				+ ", taskId=" + taskId + ", allocateId=" + allocateId + ", ioDepotType=" + ioDepotType + ", depotId="
				+ depotId + ", shopId=" + shopId + ", shopAmount=" + shopAmount + "]";
	}
	
	
}
